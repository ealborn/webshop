@model List<ArtShop.Project.Core.Models.ProductViewModel>
@{
    ViewData["Title"] = "Index";
}

<h2>Art</h2>
<div class="container">
    <div class="row">
        <div class="col-sm-6">
            @foreach (var painting in Model)
            {
                Html.RenderPartial("art", painting);
            }


        </div>
    </div>
</div>


